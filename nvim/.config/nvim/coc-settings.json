{
  "coc.preferences.formatOnSaveFiletypes": ["go", "rust"],
  "diagnostic.errorSign": "•",
  "diagnostic.warningSign": "•",
  "diagnostic.infoSign": "•",
  "diagnostic.messageTarget": "float",
  "diagnostic.enableMessage": "jump",
  "diagnostic.refreshOnInsertMode": false,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.virtualText": true,
  "eslint.probe": [
    "javascript",
    "javascript.jsx",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "markdown",
    "svelte"
  ],
  "html.autoClosingTags": false,
  "javascript.format.enable": false,
  "typescript.format.enable": false,
  "typescript.preferences.importModuleSpecifier": "shortest",
  "javascript.preferences.importModuleSpecifier": "shortest",
  "typescript.suggest.includeAutomaticOptionalChainCompletions": true,
  "javascript.suggest.includeAutomaticOptionalChainCompletions": true,
  "typescript.inlayHints.variableTypes.enabled": true,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "javascript.inlayHints.parameterNames.enabled": "all",

  "svelte.enable-ts-plugin": true,
  "svelte.plugin.svelte.useNewTransformation": true,

  "rust-analyzer.rustfmt.extraArgs": ["+nightly"],
  "rust-analyzer.checkOnSave.enable": true,
  "rust-analyzer.checkOnSave.command": "clippy",
  "rust-analyzer.inlayHints.typeHints.enable": true,
  "rust-analyzer.completion.callable.snippets": "none",

  "workspace.rootPatterns": [".git", ".hg", "package.json", "Cargo.toml"],
  "workspace.ignoredFolders": ["$HOME", "~/Documents/cv/code", "~/cv/code"],

  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "constructor": "\uf0ad",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  "suggest.noselect": true,
  "suggest.enablePreselect": false,
  "suggest.virtualText": false,
  "suggest.maxCompleteItemCount": 20
}
